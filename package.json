    "watch": "rollup --watch --config rollup.config.dev.js",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bandaloo/gmtk2021.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bandaloo/gmtk2021/issues"
  },
  "homepage": "https://github.com/bandaloo/gmtk2021#readme",
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix --max-warnings=0"
    ]
  },
  "contributors": [
    {
      "name": "Joseph Petitti",
      "url": "https://josephpetitti.com",
      "email": "jppetitti@wpi.edu"
    }
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.2.1",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "phaser": "^3.55.2",
    "rollup": "^2.51.2",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.1.3"
  }
